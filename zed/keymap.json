[
  {
    "context": "Editor",
    "bindings": {
      "ctrl-alt-down": [
        "editor::AddSelectionBelow",
        {
          "skip_soft_wrap": true,
        },
      ],
    },
  },
  {
    "context": "Editor",
    "bindings": {
      "ctrl-alt-up": [
        "editor::AddSelectionAbove",
        {
          "skip_soft_wrap": true,
        },
      ],
    },
  },
  {
    "context": "Editor",
    "bindings": {
      "alt-shift-up": "editor::DuplicateLineUp",
    },
  },
  {
    "context": "Editor",
    "bindings": {
      "alt-shift-down": "editor::DuplicateLineDown",
    },
  },
]
